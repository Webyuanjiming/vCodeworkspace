<div ui-content-for="back">
    <a href="javascript:window.history.go(-1)"><img src="/m/images/back_24.png" /></a>
</div>
<div ui-content-for="title">
    <span>回款计划日历</span>
</div>
<div class="scrollable">
    <div class="scrollable-content section">

        <div class="scroll-calendar">
            <div class="lend">

                <div class="ler_top">
                    <div class="igs" ng-click="igr(timesMon)"></div>
                </div>

                <div class="ler_cont">
                    <div class="ler_one">
                        {{reverAll}}
                    </div>
                    <div class="ler_two">
                        计划总额（元）
                    </div>
                </div>

                <div class="ler_bottom">
                    <div class="ler_bt_left t_br">
                        <div class="ler_lt">
                            <ul>
                                <li>
                                    <p class="li-lt">{{caLendar[0].recoverYes}}</p>
                                </li>
                                <li>
                                    <p class="li-rt">本月已回款</p>
                                </li>
                            </ul>
                        </div>
                    </div>
                    <div class="ler_bt_right">
                        <div class="ler_lt">
                            <ul>
                                <li>
                                    <p class="li-lt">{{noRever}}</p>
                                </li>
                                <li>
                                    <p class="li-rt">未回款</p>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>

            </div>
            <div class="cr-xot">
                <p class="xo-p">无回款记录</p>
            </div>
            <div class="lend-box">
                <div class="lend-cort">
                    回款明细
                </div>
                <div class="lend-list">
                    <table class="lend-table">
                        <thead>
                        <tr>
                            <th>时间</th>
                            <th>已回款</th>
                            <th>未回款</th>
                            <th>总金额</th>
                        </tr>
                        </thead>
                    </table>
                </div>
                <table class="ld-table">
                    <tbody>
                        <tr ng-repeat="lend in caLendar">
                            <th>{{lend.recoverMonth}}</th>
                            <th>{{lend.recoverYes}}</th>
                            <th class="wei_cor">{{lend.recoverWait}}</th>
                            <th>{{lend.recoverTotal}}<abbr class="blt" ng-click="details(lend.recoverMonth)"></abbr></th>
                        </tr>
                      <!--  <tr>
                            <th>11月</th>
                            <th>0.00</th>
                            <th>2000.32</th>
                            <th>2000.32<abbr class="blt"></abbr></th>
                        </tr>
                        <tr>
                            <th>10月</th>
                            <th>0.00</th>
                            <th>2000.32</th>
                            <th>2000.32<abbr class="blt"></abbr></th>
                        </tr>
                        <tr>
                            <th>9月</th>
                            <th>0.00</th>
                            <th>2000.32</th>
                            <th>2000.32<abbr class="blt"></abbr></th>
                        </tr>
                        <tr>
                            <th>8月</th>
                            <th>0.00</th>
                            <th>2000.32</th>
                            <th>2000.32<abbr class="blt"></abbr></th>
                        </tr>-->
                    </tbody>
                </table>
                <div class="cor_len"></div>
            </div>
        </div>

        <div ui-content-for="modals">
            <div ng-include="'/m/jujin/common.html'"></div>
        </div>

    </div>
</div>
